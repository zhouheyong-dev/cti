{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "InboundSocket": "Debug"
    }
  },
  "Database": {
    "Cti": "${CTI_DSN}",
    "Sso": "${SSO_DSN}",
    "Freeswitch": "${FS_DSN}"
  },
  "Esl": {
    "Host": "${ESL_HOST}",
    "Port": "${ESL_PORT}",
    "Password": "${ESL_PASSWORD}"
  },
  "SecretKey": "${SECRET_KEY}",
  "Redis": "${REDIS_DSN}",
  "MQ": {
    "Host": "${MQ_HOST}",
    "Port": "${MQ_PORT}"
  },
  "Monitor": {
    "Secret": "${MONITOR_SECRET}"
  },
  "Custom": "/etc/freeswitch/custom/",
  "Record": "/etc/freeswitch/custom/record/",
  "Sounds": "",
  "AllowedHosts": "*"
}
